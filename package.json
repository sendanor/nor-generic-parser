{
  "name": "nor-generic-parser",
  "version": "1.0.0",
  "description": "Base implementation for parsers",
  "main": "dist/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha",
    "compile": "babel src --out-dir dist/src --presets=es2015",
    "compile-examples": "babel examples --out-dir dist/examples --presets=es2015",
    "compile-watch": "babel --watch src --out-dir dist/src --presets=es2015",
    "compile-docs": "jsdoc --verbose --pedantic -c jsdoc.json",
    "prepublish": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sendanor/nor-generic-parser.git"
  },
  "keywords": [
    "parser"
  ],
  "author": "Jaakko-Heikki Heusala <jheusala@iki.fi>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sendanor/nor-generic-parser/issues"
  },
  "homepage": "https://github.com/sendanor/nor-generic-parser#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "docdash": "^0.4.0",
    "jsdoc": "^3.5.5",
    "nyc": "^11.2.1",
    "mocha": "^3.5.3"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "nor-debug": "^1.0.0"
  }
}
